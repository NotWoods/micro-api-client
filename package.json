{
  "name": "micro-api-client",
  "description": "Micro REST API client",
  "version": "3.2.0",
  "author": "Mathias Biilmann Christensen",
  "babel": {
    "presets": "env",
    "plugins": [
      "syntax-object-rest-spread",
      "transform-object-rest-spread",
      [
        "transform-builtin-extend",
        {
          "globals": [
            "Error",
            "Array"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-plugin-syntax-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^7.3.0"
  },
  "homepage": "https://github.com/netlify/micro-api-client-lib",
  "bugs": "https://github.com/netlify/micro-api-client-lib/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netlify/micro-api-client-lib.git"
  },
  "keywords": [
    "fetch",
    "netlify",
    "api"
  ],
  "files": [
    "lib"
  ],
  "license": "ISC",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel src -d lib",
    "prepublish": "npm run compile"
  }
}
